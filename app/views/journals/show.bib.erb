<% bibliography = BibTeX::Bibliography.new %>

<% unless @journal.articles.empty? %>
  <% @journal.articles.each do |article| %>
    <% unless article.to_bib.nil? %>
      <% bibliography << article.to_bib %>
    <% end %>
  <% end %>
<% end %>
<%= bibliography %>